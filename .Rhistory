makeVector <- function(x = numeric()) {
m <- NULL
set <- function(y) {
x <<- y
m <<- NULL
}
get <- function() x
setmean <- function(mean) m <<- mean
getmean <- function() m
list(set = set, get = get,
setmean = setmean,
getmean = getmean)
}
cachemean <- function(x, ...) {
m <- x$getmean()
if(!is.null(m)) {
message("getting cached data")
return(m)
}
data <- x$get()
m <- mean(data, ...)
x$setmean(m)
m
}
makeVector(3:5)
x <- c(3:5)
x
makeVector(x)
y <- x
makeVector(x)
m <- NULL
makeVector(x)
makeVector(3)
cachemean(x)
cachemean(1)
makeVector(10:3)
makeVector <- function(x = numeric()) {
m <- NULL
set <- function(y) {x <<- y; m <<- NULL}
get <- function() x
cachemean <- function (...) {
if(is.null(m)) {
message("Recaculating mean!")
m <<- mean(x,...)
}
m
}
list(set = set, get = get, cachemean = cachemean)
}
makeVector(10:3)
